{
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "prettier": true,
      "ordered-imports": [
        false
      ],
      "quotemark": [
        true,
        "single",
        "jsx-single",
        "avoid-escape"
      ],
      "semicolon": [
        false,
        "never"
      ],
      "member-access": [
        false
      ],
      "member-ordering": [
        false
      ],
      "trailing-comma": [
        true,
        {
          "singleline": "never",
          "multiline": "always"
        }
      ],
      "no-empty": false,
      "no-submodule-imports": false,
      "no-implicit-dependencies": false,
      "no-constant-condition": false,
      "triple-equals": [
        true,
        "allow-undefined-check"
      ],
      "ter-indent": [
        false,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-duplicate-imports": true,
      "jsx-alignment": false,
      "jsx-no-bind": true,
      "jsx-no-lambda": true,
      "interface-name": false,
      "object-literal-sort-keys": false,
      "max-classes-per-file": false
    }
  }
